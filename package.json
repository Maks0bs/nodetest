{
  "name": "apitest1",
  "version": "1.0.0",
  "description": "A nodejs api",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "client": "cd client && yarn start",
    "server": "nodemon app.js",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false cd client && npm install && npm run build"
    
  },
  "keywords": [
    "node",
    "api"
  ],
  "author": "maks0bs",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "express-validator": "^5.3.1",
    "formidable": "^1.2.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.15",
    "mongoose": "^5.8.11",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.5",
    "nodemon": "^2.0.2",
    "uuid": "^3.4.0"
  }
}
